<ul>
  <% @memories.each do |memorie| %>
    <li><%= memorie.name %></li>
  <% end %>
</ul>
<%= form_with model: @memorie do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.number_field :description %>
  <%= f.label :created_at %>
  <%= f.text_field :created_at %>
  <%= f.label :updated_at %>
  <%= f.text_field :updated_at %>
  <%= f.submit %>
<% end %>
