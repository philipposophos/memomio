<div class="container">
  <div class="form">
    <h1>Add note to your memory "<%= @memory.title %>"</h1>

    <hr>

    <%= simple_form_for [@memory, @tag] do |f| %>
      <%= f.association :emotional_tag, collection: @emotional_tags.map {|tag| [tag["emotion"], tag["id"]]} %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
